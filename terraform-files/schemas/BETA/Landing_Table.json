{
    "schema": {
      "fields":[
        {
            "description": "Unique identifier for each record",
            "name": "record_id",
            "type": "STRING"
        },
        {
            "description": "ID of the taxi vendor",
            "name": "vendor_id",
            "type": "STRING"
        },
        {
            "description": "Timestamp when the trip started",
            "name": "pickup_datetime",
            "type": "TIMESTAMP"
        },
        {
            "description": "Timestamp when the trip ended",
            "name": "dropoff_datetime",
            "type": "TIMESTAMP"
        },
        {
            "description": "Number of passengers",
            "name": "passenger_count",
            "type": "INTEGER"
        },
        {
            "description": "Distance of the trip in miles",
            "name": "trip_distance",
            "type": "FLOAT"
        },
        {
            "description": "Longitude of pickup location",
            "name": "pickup_longitude",
            "type": "FLOAT"
        },
        {
            "description": "Latitude of pickup location",
            "name": "pickup_latitude",
            "type": "FLOAT"
        },
        {
            "description": "Longitude of dropoff location",
            "name": "dropoff_longitude",
            "type": "FLOAT"
        },
        {
            "description": "Latitude of dropoff location",
            "name": "dropoff_latitude",
            "type": "FLOAT"
        },
        {
            "description": "Fare charged for the trip",
            "name": "fare_amount",
            "type": "FLOAT"
        },
        {
            "description": "Extra charges",
            "name": "extra",
            "type": "FLOAT"
        },
        {
            "description": "MTA tax",
            "name": "mta_tax",
            "type": "FLOAT"
        },
        {
            "description": "Tip amount",
            "name": "tip_amount",
            "type": "FLOAT"
        },
        {
            "description": "Tolls amount",
            "name": "tolls_amount",
            "type": "FLOAT"
        },
        {
            "description": "Total amount charged",
            "name": "total_amount",
            "type": "FLOAT"
        },
        {
            "description": "Payment method used (e.g., card, cash)",
            "name": "payment_type",
            "type": "STRING"
        },
        {
            "description": "Indicates if the trip data was stored and forwarded",
            "name": "store_and_fwd_flag",
            "type": "STRING"
        }
      ]
    }
  }